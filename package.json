{
  "name": "src",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run minify && node server.js",
    "dev": "budo index.js --live --pushstate --base=/ --onupdate \"standard | notify-error\" -- -t babelify -t sheetify/transform",
    "build": "browserify -t [ sheetify/transform ] -t [ babelify --presets [ es2015 ] ] index.js > ./build/assets/bundle.js",
    "minify": "uglifyjs --compress --mangle --output=./build/assets/bundle.min.js -- ./build/assets/bundle.js",
    "lint": "standard",
    "unit": "tape -r babel-register **/*.unit.js | tap-spec",
    "unit/watch": "tape-watch -r babel-register **/*.unit.js | tap-spec",
    "coverage": "nyc --reporter=lcov --reporter=html npm test",
    "test": "npm run unit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "choo": "^4.0.3",
    "sheetify": "^6.0.1",
    "validate.js": "^0.11.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "budo": "^9.3.0",
    "express": "^4.14.0",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.1",
    "notify-error": "^1.2.0",
    "nyc": "^10.0.0",
    "sinon": "^1.17.6",
    "standard": "^8.6.0",
    "tachyons": "^4.6.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "uglify-js": "^2.7.5"
  }
}
